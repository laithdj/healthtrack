{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "baseUrl": ".",
    "paths": {
      "jszip": [
        "../node_modules/jszip/dist/jszip.js"
      ],
      "patient-header": [
        "dist/patient-header"
      ],
      "patient-header/*": [
        "dist/patient-header/*"
      ]
    },
    "lib": [
      "es2017",
      "dom",
      "esnext"
    ]
  },
  "include": [
    "src/**/*.ts",
    "node_modules/hms.interfaces/*.ts"
  ]
}